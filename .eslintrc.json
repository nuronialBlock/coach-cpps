{
  "extends": ["google", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "new-cap": ["error", {
      "capIsNewExceptions": ["Router"]
      }],
    "require-jsdoc": "off"
  }
}
